<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<h1>&lt;c:forEach&gt;</h1>
	<!-- begin부터 end까지 1씩 증가 -->
	<c:forEach begin="1" end="5">
		<p>hello</p>
	</c:forEach>
	
	<c:forEach begin="1" end="5" var="index">
	
	<p>${index } : hi</p>
	</c:forEach>
	
</body>
